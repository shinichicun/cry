   "source": [
    "T = 3\n",
    "def baseT(n, b=T):\n",
    "    v = []\n",
    "    while True:\n",
    "        x = n // b\n",
    "        y = n % b\n",
    "        v.append(y)\n",
    "        if x == 0:\n",
    "            break\n",
    "        n = x\n",
    "    v.reverse()\n",
    "    return v\n",
    "baseT(2**32-10)"
   ]
  }
